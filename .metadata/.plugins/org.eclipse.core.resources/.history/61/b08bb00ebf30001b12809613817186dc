
server.port=8082
server.address=127.0.0.1
# Per una best practice assegnare un nome, non lasciare quello di default
spring.application.name=client2

# Esporre tutti gi endpoint di sping boot actuator
management.endpoints.web.exposure.include=*
# Indichiamo dove si trova il server di spring boot admin
spring.boot.admin.client.url=http://localhost:8080
spring.boot.admin.client.instance.prefer-ip=true

logging.file.name=client2.log
management.endpoint.logfile.external-file=client2.log
